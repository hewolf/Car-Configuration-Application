<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Car_Configuration_Web</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>ConfigurePage.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>servletmodel</servlet-name>
		<servlet-class>servlet.Servlet_Model</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>servletmodel</servlet-name>
		<url-pattern>/servletmodel</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>servletoptions</servlet-name>
		<servlet-class>servlet.Servlet_Optionsets</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>servletoptions</servlet-name>
		<url-pattern>/servletoptions</url-pattern>
	</servlet-mapping>
	
</web-app>